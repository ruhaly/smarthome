package com.changhong.foundation.entity;

public class BusinessInfo
{
    //包名
    private String packageName;
    
    //apk主界面全路径
    private String mainActivityPath;
    
    //后台传的id
    private String apkId;
    
    //原子业务编码
    private String businessCode;
    
    //apk名字
    private String businessName;
    
    //apk大小
    private String size;
    
    //原子业务类型
    private String businessType;
    
    //权限
    //0:已购买
    //1:未购买
    //2:免费使用
    private String isAuthority;
    
    //序列号
    private String serialNumber;
    
    //安装包地址
    private String sourceUrl;
    
    //所属SP
    private String spId;
    
    //最新版本号
    private String versionNo;
    
    public String getPackageName()
    {
        return packageName;
    }
    
    public void setPackageName(String packageName)
    {
        this.packageName = packageName;
    }
    
    public String getMainActivityPath()
    {
        return mainActivityPath;
    }
    
    public void setMainActivityPath(String mainActivityPath)
    {
        this.mainActivityPath = mainActivityPath;
    }
    
    public String getSize()
    {
        return size;
    }
    
    public void setSize(String size)
    {
        this.size = size;
    }
    
    public String getApkId()
    {
        return apkId;
    }
    
    public void setApkId(String apkId)
    {
        this.apkId = apkId;
    }
    
    public String getBusinessCode()
    {
        return businessCode;
    }
    
    public void setBusinessCode(String businessCode)
    {
        this.businessCode = businessCode;
    }
    
    public String getBusinessName()
    {
        return businessName;
    }
    
    public void setBusinessName(String businessName)
    {
        this.businessName = businessName;
    }
    
    public String getBusinessType()
    {
        return businessType;
    }
    
    public void setBusinessType(String businessType)
    {
        this.businessType = businessType;
    }
    
    public String getIsAuthority()
    {
        return isAuthority;
    }
    
    public void setIsAuthority(String isAuthority)
    {
        this.isAuthority = isAuthority;
    }
    
    public String getSerialNumber()
    {
        return serialNumber;
    }
    
    public void setSerialNumber(String serialNumber)
    {
        this.serialNumber = serialNumber;
    }
    
    public String getSourceUrl()
    {
        return sourceUrl;
    }
    
    public void setSourceUrl(String sourceUrl)
    {
        this.sourceUrl = sourceUrl;
    }
    
    public String getSpId()
    {
        return spId;
    }
    
    public void setSpId(String spId)
    {
        this.spId = spId;
    }
    
    public String getVersionNo()
    {
        return versionNo;
    }
    
    public void setVersionNo(String versionNo)
    {
        this.versionNo = versionNo;
    }
    
}
