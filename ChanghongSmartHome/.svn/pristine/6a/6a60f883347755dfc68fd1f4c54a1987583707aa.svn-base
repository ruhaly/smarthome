<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <!-- 物业服务 start -->
    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/room_rating_bar</item>
        <!--
        <item name="android:minHeight">16dip</item> 
       <item name="android:maxHeight">16dip</item>
        -->
    </style>

    <style name="roomRatingBarhide" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/room_rating_bar</item>
        <item name="android:isIndicator">true</item>
    </style>
    <!-- 物业服务 end -->


    <!-- 社区影院 -->
    <style name="AppTheme" parent="android:Theme.Light" />

    <style name="MyDialog" parent="@android:Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
    </style>

    <style name="TextViewStyle">
        <item name="android:textSize">@dimen/text_size_vlarge</item>
        <item name="android:textColor">#5f5f5f</item>
        <!--
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewStyle_popup">
        <item name="android:textSize">@dimen/text_size_vlarge_popup</item>
        <item name="android:textColor">#d9d9d9</item>
        <!--
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewTitleStyle">
        <item name="android:textSize">@dimen/text_size_title</item>
        <item name="android:textColor">#5f5f5f</item>
        <!--
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewTitleStyle_write26">
        <item name="android:textSize">@dimen/text_size_title_write26</item>
        <item name="android:textColor">#d9d9d9</item>
        <!--
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewTitleStyle_write27">
        <item name="android:textSize">@dimen/text_size_title_write27</item>
        <item name="android:textColor">#d9d9d9</item>
        <!--
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="TextViewTitleStyle_write">
        <item name="android:textSize">@dimen/text_size_title_write</item>
        <item name="android:textColor">#d9d9d9</item>
        <!--
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        -->
        <item name="android:background">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ThemeActivity" mce_bogus="1">
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="AnimationActivity" mce_bogus="1" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/anim_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/anim_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/back_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/back_exit</item>
    </style>

    <style name="AnimationFade">

        <!-- PopupWindow左右弹出的效果 -->
        <item name="android:windowExitAnimation">@anim/in_left_to_right</item>
        <item name="android:windowEnterAnimation">@anim/out_right_to_left</item>
    </style>

    <style name="roomRatingBarCinema" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/room_rating_bar</item>
        <item name="android:isIndicator">true</item>
        <item name="android:maxHeight">30dip</item>
        <item name="android:maxHeight">30dip</item>
    </style>

    <style name="MediaController_SeekBar" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:indeterminateDrawable">@drawable/scrubber_progress_horizontal_holo_dark</item>
        <item name="android:minHeight">3dip</item>
        <item name="android:maxHeight">3dip</item>
        <item name="android:thumb">@drawable/play_prog</item>
        <!-- <item name="android:thumbOffset">16dip</item> -->
        <!--
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        -->
    </style>

    <style name="MediaController_Text">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- 社区影院 -->


    <!-- 社区黄页 -->
    <style name="TextViewWordStyle">
        <item name="android:textColor">#f25454</item>
        <item name="android:textSize">25dip</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextViewTelephoneStyle">
        <item name="android:textColor">#919191</item>
        <item name="android:textSize">15dip</item>
        <item name="android:textStyle">bold</item>
    </style>
    <!-- 社区黄页 -->


    <!-- 积分 -->
    <style name="TextViewIntegrationLittleTitleStyle">
        <item name="android:textColor">#5e5f5e</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="TextViewIntegrationStyle">
        <item name="android:textColor">#5e5e5e</item>
        <item name="android:textSize">15sp</item>
    </style>
    
    <style name="TextViewIntegrationMyChangeStyle">
        <item name="android:textColor">#5e5e5e</item>
        <item name="android:textSize">18sp</item>
    </style>

    <!-- 积分 -->


    <!-- 商圈 -->
    <style name="shopping_rating_bar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/shopping_rating_bar</item>
    </style>

    <style name="order_rating_bar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/order_rating_bar</item>
    </style>

    <style name="order_info_rating_bar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/order_info_rating_bar</item>
    </style>
    <!-- 商圈 -->

</resources>