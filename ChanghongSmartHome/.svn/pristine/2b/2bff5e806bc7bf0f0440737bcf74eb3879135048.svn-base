public class IntegrationDetailVO
{
	private int peResult;
	
    private long totalPoint;
    
    private List<IntegrationChangeItemVO> pointDetailList;
    
	public int getPeResult() {
		return peResult;
	}
	
	public void setPeResult(int peResult) {
		this.peResult = peResult;
	}
	
	public long getTotalPoint() {
		return totalPoint;
	}
	
	public void setTotalPoint(long totalPoint) {
		this.totalPoint = totalPoint;
	}
	
	public List<IntegrationChangeItemVO> getPointDetailList() {
		return pointDetailList;
	}
	
	public void setPointDetailList(List<IntegrationChangeItemVO> pointDetailList) {
		this.pointDetailList = pointDetailList;
	}
    
}

