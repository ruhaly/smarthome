<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include
        android:layout_width="fill_parent"
        android:layout_height="48dip"
        layout="@layout/title_yellowpages_top" >
    </include>

    <ScrollView
        android:id="@+id/ScrollView_mainright"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/bg" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:padding="5dip"            
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="150dip"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/toflinearlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="120dip"
                        android:background="@drawable/advertise_takeout" />

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent" >

                        <ImageView
                            android:id="@+id/takeoutFoodView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_takeout" />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/takeoutFoodView"
                            android:layout_alignRight="@id/takeoutFoodView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:text="@string/takeoutfood" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dip"
                    android:layout_marginLeft="10dip"
                    android:layout_weight="2"
                    android:orientation="vertical" >

                    <RelativeLayout
                        android:id="@+id/psreativelayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <ImageView
                            android:id="@+id/propertyserviceView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bg_propertyservice" />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="5dip"
                            android:text="@string/propertyservice" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/propertyserviceView"
                            android:layout_alignRight="@id/propertyserviceView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:background="@drawable/icon_propertyservice" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/hhsrelativelayout"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginTop="10dip"
                        android:layout_weight="1" >

                        <ImageView
                            android:id="@+id/householdserviceView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:background="@drawable/bg_householdservice" />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="5dip"
                            android:text="@string/householdservice" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/householdserviceView"
                            android:layout_alignRight="@id/householdserviceView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:background="@drawable/icon_householdservice" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="70dip"
                android:layout_marginTop="10dip"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:orientation="horizontal"
                    android:layout_weight="1" >

                    <RelativeLayout
                        android:id="@+id/mrelativelayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <ImageView
                            android:id="@+id/maintainView"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg_maintain"
                             />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="5dip"
                            android:text="@string/maintain" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/maintainView"
                            android:layout_alignRight="@id/maintainView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:background="@drawable/icon_maintain" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/urelativelayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dip"
                        android:layout_weight="1" >

                        <ImageView
                            android:id="@+id/unlockView"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg_unlock"
                            />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="5dip"
                            android:text="@string/unlock" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/unlockView"
                            android:layout_alignRight="@id/unlockView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:background="@drawable/icon_unlock" />
                    </RelativeLayout> 
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/drelativelayout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dip"
                    android:layout_weight="2" >

                    <ImageView
                        android:id="@+id/dredgeView"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="@drawable/bg_dredge" />

                    <TextView
                        style="@style/TextViewTitleStyle_write26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dip"
                        android:layout_marginTop="5dip"
                        android:text="@string/dredge" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/dredgeView"
                        android:layout_alignRight="@id/dredgeView"
                        android:layout_marginBottom="5dip"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/icon_dredge" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="150dip"
                android:layout_marginTop="10dip"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/aetlinearlayout"
                    android:layout_width="match_parent"
                    android:layout_height="150dip"
                    android:orientation="vertical"
                    android:layout_weight="1" >

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="120dip"
                        android:background="@drawable/advertise_edutrain" />

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" >

                        <ImageView
                            android:id="@+id/edutrainView"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg_edutrain" />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/edutrainView"
                            android:layout_alignRight="@id/edutrainView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:text="@string/edutrain" />
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/dprelativelayout"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="10dip"
                    android:layout_weight="2" >

                    <ImageView
                        android:id="@+id/dispatchView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/bg_dispatch" />

                    <TextView
                        style="@style/TextViewTitleStyle_write26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dip"
                        android:layout_marginTop="5dip"
                        android:text="@string/dispatch" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/dispatchView"
                        android:layout_alignRight="@id/dispatchView"
                        android:layout_marginBottom="5dip"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/icon_dispatch" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="150dip"
                android:layout_marginTop="10dip"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@+id/hrelativelayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1" >

                    <ImageView
                        android:id="@+id/hospitalView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/bg_hospital" />

                    <TextView
                        style="@style/TextViewTitleStyle_write26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dip"
                        android:layout_marginTop="5dip"
                        android:text="@string/hospital" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/hospitalView"
                        android:layout_alignRight="@id/hospitalView"
                        android:layout_marginBottom="15dip"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/icon_hospital" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/bsrelativelayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1" >

                    <ImageView
                        android:id="@+id/busserviceView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dip"
                        android:background="@drawable/bg_busservice" />

                    <TextView
                        style="@style/TextViewTitleStyle_write26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dip"
                        android:layout_marginTop="5dip"
                        android:text="@string/busservice" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/busserviceView"
                        android:layout_alignRight="@id/busserviceView"
                        android:layout_marginBottom="5dip"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/icon_busservice" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="70dip"
                android:layout_marginTop="10px"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="70dip"
                    android:layout_weight="1"
                    android:orientation="horizontal" >

                    <RelativeLayout
                        android:id="@+id/mhrealtivelayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <ImageView
                            android:id="@+id/movehouseView"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg_movehouse" />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="5dip"
                            android:text="@string/movehouse" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/movehouseView"
                            android:layout_alignRight="@id/movehouseView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:background="@drawable/icon_movehouse" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/trelativelayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1" >

                        <ImageView
                            android:id="@+id/taxiView"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dip"
                            android:background="@drawable/bg_taxi" />

                        <TextView
                            style="@style/TextViewTitleStyle_write26"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dip"
                            android:layout_marginTop="5dip"
                            android:text="@string/taxi" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignBottom="@id/taxiView"
                            android:layout_alignRight="@id/taxiView"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="5dip"
                            android:background="@drawable/icon_taxi" />
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/osrelativelayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="2" >

                    <ImageView
                        android:id="@+id/othersView"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dip"
                        android:layout_weight="2"
                        android:background="@drawable/bg_others" />

                    <TextView
                        style="@style/TextViewTitleStyle_write26"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dip"
                        android:layout_marginTop="5dip"
                        android:text="@string/others" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@id/othersView"
                        android:layout_alignRight="@id/othersView"
                        android:layout_marginBottom="5dip"
                        android:layout_marginRight="5dip"
                        android:background="@drawable/icon_elese" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>